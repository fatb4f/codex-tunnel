{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "type": "filesystem",
    "path": "codex_bridge.py",
    "entrypoint": "create_server"
  },
  "environment": {
    "type": "uv",
    "project": ".",
    "dependencies": [
      "fastmcp>=2,<3",
      "python-dotenv>=1.0"
    ]
  },
  "deployment": {
    "transport": "streamable-http",
    "host": "0.0.0.0",
    "port": 8000,
    "path": "/mcp/",
    "cwd": ".",
    "log_level": "INFO"
  }
}
